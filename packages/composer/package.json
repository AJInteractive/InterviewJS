{
  "name": "interviewjs-composer",
  "description": "",
  "version": "0.0.0",
  "author": "",
  "homepage": "http://interviewjs.io/composer/",
  "dependencies": {
    "date-fns": "^1.29.0",
    "interviewjs-styleguide": "*",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-app-rewire-babel-loader": "0.1.1",
    "react-dom": "16.2.0",
    "react-modal": "3.2.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.1.1",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-devtools-extension": "2.13.2",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "styled-components": "3.0.2",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.18.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.6.1",
    "gh-pages": "1.1.0",
    "husky": "0.14.3",
    "lint-staged": "6.1.1",
    "node-plop": "0.9.0",
    "plop": "1.9.1",
    "prettier": "1.10.2",
    "react-app-rewired": "1.4.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "prettier": "prettier --single-quote --print-width=120 --trailing-comma es5",
    "format": "npm run prettier -- --write '{,!(build|generators)/**/}*.js'",
    "precommit": "lint-staged",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint **/*.js --quiet",
    "lint-fix": "eslint --fix",
    "generate": "plop --plopfile generators/index.js",
    "release": "npm run build && gh-pages -d build -e composer -a"
  },
  "lint-staged": {
    "{,!(build|generators)/**/}*.js": [
      "npm run prettier -- --write",
      "git add"
    ]
  },
  "engines": {
    "npm": ">=4",
    "node": ">=6"
  }
}
